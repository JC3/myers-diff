{
  "name": "myers-diff",
  "author": "Jamie Peabody",
  "version": "1.0.5",
  "description": "Implementation of the longest common subsequence (diff) algorithm.",
  "main": "src/index.js",
  "keywords": [
    "diff",
    "merge",
    "longest-common-subsequence",
    "shortest-middle-snake",
    "compare",
    "patch"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wickedest/myers-diff.git"
  },
  "bugs": {
    "url": "https://github.com/wickedest/myers-diff/issues"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov"
    ]
  },
  "homepage": "https://github.com/wickedest/myers-diff",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run lint && npm run test",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha --recursive",
    "test:nocoverage": "mocha --recursive",
    "lint": "eslint src test"
  }
}
